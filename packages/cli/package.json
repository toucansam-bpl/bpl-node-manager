{
  "name": "bpl-cli",
  "description": "Pluggable Blockpool Node Manager",
  "version": "1.0.8-alpha.0",
  "author": "toucansam",
  "bin": {
    "bpl": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-autocomplete": "^0.1.0",
    "@oclif/plugin-help": "^2.1.6",
    "@oclif/plugin-not-found": "^1.2.2",
    "@oclif/plugin-plugins": "^1.7.8",
    "@toucansam-bpl/cli-core": "file:../cli-core",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "nyc": "^14.0.0",
    "riteway": "^6.0.3",
    "tap-nirvana": "^1.1.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "watch": "^1.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bpl",
    "scope": "toucansam-bpl",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "clean": "rm -Rf lib",
    "compile": "tsc -b --force",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b --force && oclif-dev manifest && oclif-dev readme",
    "test": "echo \"cli does not have tests yet\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
